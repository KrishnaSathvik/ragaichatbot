{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python"
    },
    "api/test.py": {
      "runtime": "@vercel/python"
    },
    "api/health.py": {
      "runtime": "@vercel/python"
    },
    "api/chat.py": {
      "runtime": "@vercel/python"
    },
    "api/transcribe.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ]
}
